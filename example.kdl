fuzzel-args --minimal-lines

program Firefox {
    command firefox
    icon firefox
}

program Vesktop {
    command flatpak run dev.vencord.Vesktop
    icon dev.vencord.Vesktop
}

menu Submenu {
    fuzzel-args --minimal-lines --hide-prompt
    icon-dir "~/.config/uff/"
    icon tbh

    program Yippee {
        command notify-send yippee
        icon tbh
    }
}
